cli editor vi
cli prompt host status multi-line
cli confirm-edit disabled
cli keymap default
#cli tcl-syntax-highlighting enabled
alias tcpdump run util tcpdump
alias ping run util ping
alias ping6 run util ping6
alias exit quit
alias showtmm show sys tmm-traffic | grep -E 'Sys|--|Conn' | grep -v -E 'Mem|Red'
alias showtmmmem show sys tmm-info | grep -E 'Sys|--|id|Mem|Avail|Used|CPU|Last'
alias huptmm restart sys service tmm
alias ssc save sys config
